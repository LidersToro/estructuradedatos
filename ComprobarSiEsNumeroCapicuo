#include "stdafx.h"
#include <iostream>
#include <conio.h>
using namespace std;

void main (){
	int n,s,t,ne;
	t=0;
	cin>>n;
	ne=n;
	while (n>0)
	{
	  s=n%10;
	  t=t*10+s;
	  n=n/10;

	}
	if (ne==t){
		cout<<t<<" es un numero capicuo";
	}
	else{
		cout<<t<<" no es un numero capicuo";
	}
	getch();
}
